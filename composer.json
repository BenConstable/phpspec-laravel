{
    "name": "benconstable/phpspec-laravel",
    "description": "Test your Laravel applications with PhpSpec",
    "keywords": ["laravel", "phpspec", "testing", "test"],
    "homepage": "http://github.com/BenConstable/phpspec-laravel",
    "type": "library",
    "authors": [
        {
            "name": "Ben Constable",
            "email": "ben@benconstable.co.uk",
            "homepage": "http://www.benconstable.co.uk"
        }
    ],
    "license": "MIT",
    "require": {
        "phpspec/phpspec": "2.1.x-dev"
    },
    "suggest": {
        "laravel/laravel": "Install laravel/laravel:dev-develop or a stable version of laravel 5.0 to use it as the framework_path."
    },
    "autoload": {
        "psr-0": {
            "PhpSpec": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin"
    }
}
