{
    "name": "benconstable/phpspec-laravel",
    "description": "Test your Laravel applications with PhpSpec",
    "keywords": ["laravel", "phpspec", "testing", "test"],
    "homepage": "http://github.com/BenConstable/phpspec-laravel",
    "type": "library",
    "authors": [
        {
            "name": "Ben Constable",
            "email": "ben@benconstable.co.uk",
            "homepage": "http://www.benconstable.co.uk"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.3.0",
        "laravel/framework": "~4.1 | ~5.0",
        "phpspec/phpspec": "~2.0"
    },
    "autoload": {
        "psr-0": {
            "PhpSpec": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
